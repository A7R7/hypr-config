
    "layer": "top",

    "position": "top",
    // If height property would be not present, it'd be calculated dynamically
    "height": 20,
    /* "margin-left":5, */
    /* "margin-right":5, */
    /* "margin-top":5, */
    /* "margin-bottom":6, */
    
    "modules-left": [
        /* "sway/workspaces", */
        "custom/spaceleft",
        "wlr/workspaces",
        "custom/spaceright",
        "hyprland/window",
    ],
    "modules-center": [
    ],
    "modules-right": [
        "custom/asd2num",
        "disk",
        "network",
        "cpu",
        "memory",
        //"temperature",
        "pulseaudio",
        "battery",
        //"clock#date",
        "clock#time",
        "tray",
    ],

    "custom/spaceleft":{
        //"format": "",
        "format": "⭘",
        "on-click": "wlogout",
    },

    "custom/spaceright":{
        "format": "",
        "on-click": "rofi -theme slate.rasi -show combi -show-icons",
        
    },

    "custom/asd2num":{
        "format": "{} ",
        "exec" : "source /home/wadekiny/Scripts/keyd/asd2num_flag.txt && echo $_asd2num",
        "interval":1,
    },

    "disk": {
        "interval": 30,
        "format": " {free}",
        "path": "/"
    },

    "battery": {
        "interval": 60,
        "states": {
            "warning": 30,
            "critical": 15
        },
        // Connected to AC
        /* "format": "  {icon}  {capacity}%", // Icon: bolt */
        "format": " {capacity}%", // Icon: bolt

        // Not connected to AC
        "format-discharging": "{icon} {capacity}%",
        "format-icons": [
            /* "", // Icon: battery-full */
            /* "", // Icon: battery-three-quarters */
            /* "", // Icon: battery-half */
            /* "", // Icon: battery-quarter */
            /* ""  // Icon: battery-empty */
            "",
            "",
            "",
            "",
            "",
        ],
        "tooltip": false
    },

